{
  "name": "chessboard-vanilla",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others --prefix \"{name}\" --names \"VITE,TUNNEL\" \"vite\" \"sleep 3 && ngrok http 5173 --domain=eager-terrier-especially.ngrok-free.app\"",
    "dev:local": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "ngrok": "ngrok http 5173",
    "ngrok:url": "curl -s http://localhost:4040/api/tunnels | grep -o '\"public_url\":\"[^\"]*\"' | cut -d'\"' -f4",
    "tunnel:url": "echo 'Permanent URL: https://eager-terrier-especially.ngrok-free.app'",
    "cloudflared": "cloudflared tunnel --url http://localhost:5173 --no-autoupdate"
  },
  "dependencies": {
    "chess.js": "^1.4.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "stockfish.js": "^10.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.4"
  }
}
