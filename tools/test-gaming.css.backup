/* Test gaming themes CSS file */

.theme-cyber {
  --background: #0a0a0a;
  --foreground: #00ff00;
  --primary: #00ffff;
  --secondary: #ff00ff;
  --accent: #ffff00;
  --radius: 0.25rem;
}

.theme-neon {
  --background: #000000;
  --foreground: #ffffff;
  --primary: #ff0080;
  --secondary: #8000ff;
  --accent: #00ff80;
  --radius: 0.5rem;
}